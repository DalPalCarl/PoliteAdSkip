{
    "manifest_version": 3,
    "name": "Polite Ad Mute",
    "description": "Ad-muting extension to automatically mute ads on Youtube",
    "version": "0.1.0",
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/PoliteSkip.png"
    },

    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "https://www.youtube.com/watch*"
            ]
        }
    ],

    "icons": {
        "16": "images/skip_16.png",
        "32": "images/skip_32.png",
        "64": "images/skip_64.png",
        "128": "images/skip_128.png"
    }
  }